    <dependencies>

        <!-- Lombok Dependency -->
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <version>1.18.4</version>
        </dependency>

        <!-- Google Gson Dependency -->
        <dependency>
            <groupId>com.google.code.gson</groupId>
